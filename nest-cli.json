{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/task-management-system/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/task-management-system/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/task-management-system",
  "projects": {
    "attachment-service": {
      "type": "application",
      "root": "apps/attachment-service",
      "entryFile": "main",
      "sourceRoot": "apps/attachment-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/attachment-service/tsconfig.app.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "job-service": {
      "type": "application",
      "root": "apps/job-service",
      "entryFile": "main",
      "sourceRoot": "apps/job-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/job-service/tsconfig.app.json"
      }
    },
    "monitoring-service": {
      "type": "application",
      "root": "apps/monitoring-service",
      "entryFile": "main",
      "sourceRoot": "apps/monitoring-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/monitoring-service/tsconfig.app.json"
      }
    },
    "task-management-system": {
      "type": "application",
      "root": "apps/task-management-system",
      "entryFile": "main",
      "sourceRoot": "apps/task-management-system/src",
      "compilerOptions": {
        "tsConfigPath": "apps/task-management-system/tsconfig.app.json"
      }
    },
    "task-service": {
      "type": "application",
      "root": "apps/task-service",
      "entryFile": "main",
      "sourceRoot": "apps/task-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/task-service/tsconfig.app.json"
      }
    },
    "user-service": {
      "type": "application",
      "root": "apps/user-service",
      "entryFile": "main",
      "sourceRoot": "apps/user-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-service/tsconfig.app.json"
      }
    }
  }
}